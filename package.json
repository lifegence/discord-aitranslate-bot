{
  "name": "discord-realtime-translator",
  "version": "1.0.0",
  "description": "Discord bot for real-time voice translation using Google Gemini API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "discord",
    "bot",
    "translation",
    "gemini",
    "voice",
    "realtime"
  ],
  "author": "Lifegence Corporation.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifegence/discord-aitranslate-bot.git"
  },
  "bugs": {
    "url": "https://github.com/lifegence/discord-aitranslate-bot/issues"
  },
  "homepage": "https://github.com/lifegence/discord-aitranslate-bot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.17.0",
    "@google/generative-ai": "^0.21.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "@discordjs/opus": "^0.9.0",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "prism-media": "^1.3.5",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.9.0",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
